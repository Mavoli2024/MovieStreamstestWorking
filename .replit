modules = ["nodejs-20", "web", "postgresql-16"]
run = "node index.js"
[agent]
expertMode = true

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Madifa Streaming Server"

[[workflows.workflow]]
name = "Madifa Streaming Server"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npx tsx server/index.ts"
